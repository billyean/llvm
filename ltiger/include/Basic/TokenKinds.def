#ifndef TOK
#define TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_ ## ID)
#endif
TOK(unknown)
TOK(eof)
TOK(identifier)
TOK(int_lit)
TOK(float_lit)
PUNCTUATOR(comma,      ",")
PUNCTUATOR(dot,        ".")
PUNCTUATOR(colon,      ":")
PUNCTUATOR(semicolon,  ";")
PUNCTUATOR(openparen,  "(")
PUNCTUATOR(closeparen, ")")
PUNCTUATOR(openbrack,  "[")
PUNCTUATOR(closebrack, "]")
PUNCTUATOR(opencurly,  "{")
PUNCTUATOR(closecurly, "}")
PUNCTUATOR(plus,       "+")
PUNCTUATOR(minus,      "-")
PUNCTUATOR(mult,       "*")
PUNCTUATOR(div,        "/")
PUNCTUATOR(pow,        "**")
PUNCTUATOR(equal,      "==")
PUNCTUATOR(nequal,     "!=")
PUNCTUATOR(less,       "<")
PUNCTUATOR(great,      ">")
PUNCTUATOR(lesseq,     "<=")
PUNCTUATOR(greateq,    ">=")
PUNCTUATOR(AND,        "&")
PUNCTUATOR(OR,         "|")
PUNCTUATOR(assign,     ":=")
PUNCTUATOR(tassign,    "=")
KEYWORD(array, KEYALL)
KEYWORD(begin, KEYALL)
KEYWORD(break, KEYALL)
KEYWORD(do,    KEYALL)
KEYWORD(else,  KEYALL)
KEYWORD(end,   KEYALL)
KEYWORD(enddo, KEYALL)
KEYWORD(endif, KEYALL)
KEYWORD(float, KEYALL)
KEYWORD(for,   KEYALL)
KEYWORD(function, KEYALL)
KEYWORD(if,       KEYALL)
KEYWORD(in,       KEYALL)
KEYWORD(int,      KEYALL)
KEYWORD(let,      KEYALL)
KEYWORD(main,     KEYALL)
KEYWORD(of,       KEYALL)
KEYWORD(return,   KEYALL)
KEYWORD(then,     KEYALL)
KEYWORD(to,       KEYALL)
KEYWORD(type,     KEYALL)
KEYWORD(var,      KEYALL)
KEYWORD(while,    KEYALL)
#undef KEYWORD
#undef PUNCTUATOR
#undef TOK